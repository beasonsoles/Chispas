{
  "expo": {
    "name": "Chispas",
    "slug": "chispas",
    "scheme": "http",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/chispas_icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/chispas.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/chispas_icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.beatrizsonsoles.chispas",
      "permissions": [
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ]
    },
    "web": {
      "favicon": "./assets/chispas_icon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-font"
    ],
    "extra": {
      "eas": {
        "projectId": "587c6e8f-92b0-44b9-aa33-541232f421b2"
      }
    },
    "owner": "beatrizsonsoles"
  }
}
